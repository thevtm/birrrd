<h1>User#show</h1>
<%= @user.name %>
<%= @user.email %>
</br>
<!-- Make a list of birds appear here -->
<ul>
  <% @user.birds_for_rent.each do |bird| %>
    <li>
      <%= bird.name %>
      <%= bird.location %>
      <%= bird.price %>
      <%= link_to "link", bird %>
    </li>
  <% end %>
</ul>
<%= link_to "Back to Main", root_path  %>
